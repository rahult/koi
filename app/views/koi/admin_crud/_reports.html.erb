<div class="tab-pane" id="tab-reports">
  <div class="settings">
    <div id="index-fields">

      <% overview = @report_data.first[:class] == 'overview' %>

      <% if overview %> 
        <div class="row space-b-2">
      <% end %>

      <% @report_data.each_with_index do |report, index| %>

        <% if overview && report[:class] != 'overview' %>
          </div>
          <% overview = false %>
        <% end %>

        <%= render "report_#{report[:class]}", report: report, index: index %>
      <% end %>

    </div>
  </div>

  <div id="modal-for-reports" class="modal fade hide-element as-clr">&nbsp;</div>
</div>
